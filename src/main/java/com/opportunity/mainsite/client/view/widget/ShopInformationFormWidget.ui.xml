<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">

	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					<ui:msg description="Shop Name">Shop Name</ui:msg>
				</td>
				<td>
					<g:TextBox ui:field="shopName" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="Shop Type">Shop Type</ui:msg>
					:
				</td>
				<td>
					<g:ListBox ui:field="shopType">
						<g:item>France</g:item>
						<g:item>Brésil</g:item>
						<g:item>Allemagne</g:item>
						<g:item>Cameroun</g:item>
					</g:ListBox>
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="Street">Street</ui:msg>
					:
				</td>
				<td>
					<g:TextBox ui:field="street" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="Number">Number</ui:msg>
					:
				</td>
				<td>
					<g:TextBox ui:field="number" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="Description">Description</ui:msg>
					:
				</td>
				<td>
					<g:TextArea ui:field="description" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="City">City</ui:msg>
					:
				</td>
				<td>
					<g:TextBox ui:field="city" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="State">State</ui:msg>
					:
				</td>
				<td>
					<g:TextBox ui:field="state" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="Zip">Zip</ui:msg>
					:
				</td>
				<td>
					<g:TextBox ui:field="zip" />
				</td>
			</tr>
			<tr>
				<td>
					<ui:msg description="Country">Country</ui:msg>
					:
				</td>
				<td>
					<g:ListBox ui:field="country">
						<g:item>France</g:item>
						<g:item>Brésil</g:item>
						<g:item>Allemagne</g:item>
						<g:item>Cameroun</g:item>
					</g:ListBox>
				</td>
			</tr>

			<tr>
				<td>
					<g:Button ui:field="submitButton" text="submit" />
				</td>
				<td align="right">
					<g:Button ui:field="clear" text="clear" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>

</ui:UiBinder>